# n8n Enterprise Patch Environment Configuration
# Copy this file to .env and update with your values

# Database Configuration
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=localhost
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=your_secure_password_here
DB_POSTGRESDB_SCHEMA=public

# n8n Configuration
N8N_RUNNERS_ENABLED=true
N8N_BLOCK_ENV_ACCESS_IN_NODE=false
N8N_GIT_NODE_DISABLE_BARE_REPOS=true

# Optional: Custom webhook URL (for production)
# N8N_WEBHOOK_URL=https://your-domain.com

# Optional: Custom editor URL (for production)
# N8N_EDITOR_BASE_URL=https://your-domain.com

# Optional: Encryption key (for production)
# N8N_ENCRYPTION_KEY=your_encryption_key_here

# Optional: JWT secret (for production)
# N8N_JWT_SECRET=your_jwt_secret_here


