# Patch insights dashboard license checks
RUN find /usr/local/lib/node_modules/n8n/dist -name "*insights*" -type f | xargs grep -l "isInsightsDashboardLicensed\|isInsightsSummaryLicensed" | while read file; do \
        sed -i 's/isInsightsDashboardLicensed() { return.*}/isInsightsDashboardLicensed() { return true; }/g' "$file"; \
        sed -i 's/isInsightsSummaryLicensed() { return.*}/isInsightsSummaryLicensed() { return true; }/g' "$file"; \
        sed -i 's/this\.licenseState\.isInsightsDashboardLicensed()/true/g' "$file"; \
        sed -i 's/this\.licenseState\.isInsightsSummaryLicensed()/true/g' "$file"; \
    done || true

# COMPREHENSIVE SHARING PATCHES - Fix ALL sharing validation layers
# Patch sharing service to always allow sharing
RUN find /usr/local/lib/node_modules/n8n/dist -name "*sharing*" -type f | while read file; do \
        sed -i 's/this\.license\.isSharingEnabled()/true/g' "$file"; \
        sed -i 's/this\.licenseState\.isSharingLicensed()/true/g' "$file"; \
        sed -i 's/isSharingEnabled()/true/g' "$file"; \
        sed -i 's/isSharingLicensed()/true/g' "$file"; \
    done || true

# Patch workflow sharing controller and all workflow-related files
RUN find /usr/local/lib/node_modules/n8n/dist -name "*workflow*" -type f | xargs grep -l "isSharingEnabled\|isSharingLicensed" | while read file; do \
        sed -i 's/this\.license\.isSharingEnabled()/true/g' "$file"; \
        sed -i 's/this\.licenseState\.isSharingLicensed()/true/g' "$file"; \
        sed -i 's/isSharingEnabled()/true/g' "$file"; \
        sed -i 's/isSharingLicensed()/true/g' "$file"; \
    done || true

# Patch executions controller sharing checks
RUN find /usr/local/lib/node_modules/n8n/dist -name "*execution*" -type f | xargs grep -l "isSharingEnabled" | while read file; do \
        sed -i 's/this\.license\.isSharingEnabled()/true/g' "$file"; \
        sed -i 's/isSharingEnabled()/true/g' "$file"; \
    done || true

# Patch public API workflows service sharing checks
RUN find /usr/local/lib/node_modules/n8n/dist -name "*public-api*" -type f | xargs grep -l "isSharingEnabled" | while read file; do \
        sed -i 's/Container\.get(License)\.isSharingEnabled()/true/g' "$file"; \
        sed -i 's/isSharingEnabled()/true/g' "$file"; \
    done || true

# Patch credentials sharing controller
RUN find /usr/local/lib/node_modules/n8n/dist -name "*credential*" -type f | xargs grep -l "isSharingEnabled\|isSharingLicensed" | while read file; do \
        sed -i 's/this\.license\.isSharingEnabled()/true/g' "$file"; \
        sed -i 's/this\.licenseState\.isSharingLicensed()/true/g' "$file"; \
        sed -i 's/isSharingEnabled()/true/g' "$file"; \
        sed -i 's/isSharingLicensed()/true/g' "$file"; \
    done || true

# Patch @Licensed decorator middleware to always allow sharing
RUN find /usr/local/lib/node_modules/n8n/dist -name "*middleware*" -type f | xargs grep -l "feat:sharing\|sharing" | while read file; do \
        sed -i 's/feat:sharing/feat:sharing-unlocked/g' "$file"; \
        sed -i 's/res\.status(412)/res.status(200)/g' "$file"; \
    done || true

# Patch controller registry to always allow sharing license checks
RUN find /usr/local/lib/node_modules/n8n/dist -name "*controller*" -type f | xargs grep -l "feat:sharing" | while read file; do \
        sed -i 's/feat:sharing/feat:sharing-unlocked/g' "$file"; \
    done || true

# Enable source control with careful syntax-preserving patches
RUN find /usr/local/lib/node_modules/n8n/dist -name "*source-control*" -type f | while read file; do \
        sed -i 's/isSourceControlConnected() { return false; }/isSourceControlConnected() { return true; }/g' "$file"; \
        sed -i 's/isSourceControlLicensedAndEnabled() { return false; }/isSourceControlLicensedAndEnabled() { return true; }/g' "$file"; \
        sed -i 's/source_control_not_connected/source_control_connected/g' "$file"; \
    done || true

# Patch source control middleware to return success for connection
RUN find /usr/local/lib/node_modules/n8n/dist -name "*middleware*" -type f | xargs grep -l "source_control" | while read file; do \
        sed -i 's/source_control_not_connected/source_control_connected/g' "$file"; \
        sed -i 's/res\.status(412)/res.status(200)/g' "$file"; \
    done || true

# Enable source control components in frontend with precise replacements
RUN find /usr/local/lib/node_modules/n8n/dist -name "*frontend*" -type f | while read file; do \
        sed -i 's/isSourceControlEnabled() { return false; }/isSourceControlEnabled() { return true; }/g' "$file"; \
        sed -i 's/isSourceControlLicensed() { return false; }/isSourceControlLicensed() { return true; }/g' "$file"; \
    done || true

# Patch quota limits to be unlimited (-1)
RUN find /usr/local/lib/node_modules/n8n/dist -name "*license*" -type f | while read file; do \
        sed -i 's/getValue.*quota.*activeWorkflows.*/getValue(quota) { return -1; }/g' "$file"; \
        sed -i 's/getValue.*quota.*maxVariables.*/getValue(quota) { return -1; }/g' "$file"; \
        sed -i 's/getValue.*quota.*users.*/getValue(quota) { return -1; }/g' "$file"; \
        sed -i 's/getValue.*quota.*workflowHistoryPrune.*/getValue(quota) { return -1; }/g' "$file"; \
        sed -i 's/getValue.*quota.*maxTeamProjects.*/getValue(quota) { return -1; }/g' "$file"; \
        sed -i 's/getValue.*quota.*aiCredits.*/getValue(quota) { return -1; }/g' "$file"; \
        sed -i 's/getValue.*quota.*insights.*/getValue(quota) { return -1; }/g' "$file"; \
        sed -i 's/getValue.*quota.*evaluations.*/getValue(quota) { return -1; }/g' "$file"; \
    done || true

# Patch plan name to show Enterprise
RUN find /usr/local/lib/node_modules/n8n/dist -name "*license*" -type f | while read file; do \
        sed -i 's/getPlanName.*{.*return.*}/getPlanName() { return "Enterprise"; }/g' "$file"; \
        sed -i 's/getConsumerId.*{.*return.*}/getConsumerId() { return "enterprise-free"; }/g' "$file"; \
    done || true

# Set environment variables to disable license checks
ENV N8N_LICENSE_SERVER_URL=""
ENV N8N_LICENSE_SERVER_URL_TRIAL=""
ENV N8N_LICENSE_SERVER_URL_TRIAL_ACTIVATION=""
ENV N8N_LICENSE_SERVER_URL_TRIAL_ACTIVATION_RETRY=""

# Enable source control and configure for GitHub repository
ENV N8N_SOURCE_CONTROL_DISABLED="false"
ENV N8N_SOURCE_CONTROL_CONNECTED="true"
ENV N8N_SOURCE_CONTROL_REPOSITORY_URL="https://github.com/sagebeme/n8n.git"
ENV N8N_SOURCE_CONTROL_REPOSITORY_BRANCH="master"

# Switch back to node user
USER node

# Expose port
EXPOSE 5678

# Use the exact same configuration as the base image
ENTRYPOINT ["tini", "--", "/docker-entrypoint.sh"]
CMD []

